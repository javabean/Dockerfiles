{
  "service": {
    "name": "dnsmasq",
    "tags": ["{{env "CONSUL_TAG"}}"],
    "port": 53,
    "enableTagOverride": false,
    "checks": [
      {
        "name": "dnsmasq process",
        "script": "/bin/sh -c 'if [ `pgrep dnsmasq` -eq 1 ]; then return 0; else return 2; fi'",
        "interval": "10s",
        "timeout": "1s",
        "deregister_critical_service_after": "35s"
      }
    ]
  }
}
